<SwapChainBackgroundPanel
    x:Class="Project2.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Project2"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"    
    d:DesignWidth="1280"
    d:DesignHeight="800">
    <Grid x:Name="sgrid" VerticalAlignment="Top" Height="800">
        <Grid.Background>
            <ImageBrush ImageSource="Content/GolfImage.jpg"/>
        </Grid.Background>
        <Button x:Name="bstart" Content="Start Game" HorizontalAlignment="Left" Margin="875,319,0,0" VerticalAlignment="Top" Height="99" Width="269" Click="StartGame" Background="Black" AllowDrop="True"/>
        <Button x:Name="bstart_Copy" Content="Control" HorizontalAlignment="Left" Margin="875,451,0,0" VerticalAlignment="Top" Height="99" Width="269" Click="About" Background="Black"/>
        <Button x:Name="bstart_Copy1" Content="About" HorizontalAlignment="Left" Margin="875,588,0,0" VerticalAlignment="Top" Height="99" Width="269" Click="About" Background="Black"/>
        <Button x:Name="abutton" Content="ABOUT TTTTTTTTTTTTTTTTTTTTTTTTTAAAAAAAAAAAAAAAAAAAAAAAAAAZZZZZZZZZZZZZZZZZZZZZZZZZZZZ" HorizontalAlignment="Left" Margin="406,319,0,0" VerticalAlignment="Top" Height="231" Width="361" BorderBrush="#FFE61A1A" Click="hideAbout" Visibility="Collapsed">
            <Button.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black"/>
                    <GradientStop Color="#FFAF0BEC" Offset="0.741"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>
    </Grid>
    <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Center" Height="61" Margin="0,739,0,0" Stroke="Black" VerticalAlignment="Top" Width="2000" Visibility="Collapsed"/>
    <Slider x:Name="sldforce" LostFocus="sldforce_LostFocus" HorizontalAlignment="Left" Margin="26.962,731.575,0,0" VerticalAlignment="Top" Width="924.567" RenderTransformOrigin="0.5,0.5" Height="46.888" UseLayoutRounding="False" d:LayoutRounding="Auto" CacheMode="BitmapCache" FontFamily="Global User Interface" BorderBrush="Black" Background="Black" ValueChanged="setForce" Visibility="Collapsed" GotFocus="sldforce_GotFocus">
        <Slider.RenderTransform>
            <CompositeTransform Rotation="0.098" SkewY="-0.465" TranslateX="-13.296" TranslateY="11.96" SkewX="0.627"/>
        </Slider.RenderTransform>
    </Slider>
    <Button x:Name="btnhit" Content="HIT" HorizontalAlignment="Left" Margin="994.984,517.033,0,0"  Click="hit"   VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="108.711"  UseLayoutRounding="False" Width="190.089" d:LayoutRounding="Auto" Background="#FF23E48C" BorderBrush="#FF20DE54" Visibility="Collapsed">
        <Button.RenderTransform>
            <CompositeTransform Rotation="-0.292"/>
        </Button.RenderTransform>
    </Button>
</SwapChainBackgroundPanel>
